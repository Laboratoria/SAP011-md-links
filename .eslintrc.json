{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "script"
    },

    "rules": {
        "no-unused-vars": "warn",
        "linebreak-style": ["error", "windows"],
        "arrow-body-style": ["error", "always"],
        "no-console": "off",
        "no-else-return":"off",
        "import/extensions": "off"
    }
}
    